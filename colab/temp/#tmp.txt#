    cellText = []
    cellText.append(["Samples", "%d" % self.samples.shape[0]])
    cellText.append(["Setosa Samples", "%d" % np.sum(self.labels==self.a_label)])
    cellText.append(["Versicolor Samples", "%d" % np.sum(self.labels==self.b_label)])
    cellText.append(["Samples", "%d" % self.samples.shape[0]])
    cellText.append(["Features per Sample", "%d" % (self.samples.shape[1] - 1)])
    cellText.append(["Labels", "%s" % self.unique_labels])
    cellText.append(["Threshold", "%0.3f" % self.threshold])
    cellText.append(["Learning Rate (eta)", "%0.3f" % self.learning_rate])
    cellText.append(["Max Iterations", "%s" % self.num_iterations])
    cellText.append(["Original Bias", "%0.3f" % self.orig_weights[0]])
    cellText.append(["Original Weights", "%s" % self.orig_weights[1:]])
    cellText.append(["Current Bias", "%0.3f" % self.weights[0]])
    cellText.append(["Current Weights", "%s" % self.weights[1:]])
    cellText.append(["Iterations Completed", "%s" % self.iteration_count])
    cellText.append(["Total Misclassifications", "%s" % np.sum(self.misclassifications)])
    ]
